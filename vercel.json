{
  "version": 2,
  "builds": [
    {
      "src": "src/main.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/main.ts",
      "methods": ["GET", "POST", "PATCH", "PUT", "DELETE"]
    },
    {
      "src": "/graphql",
      "dest": "/graphql",
      "methods": ["POST"]
    }
  ],
  "functions": {
    "graphql": {
      "runtime": "vercel-node@16.x",
      "memory": 512,
      "maxDuration": 10
    }
  }
}
